<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <h1> こんにちは <%= link_to "#{current_user.name}さん", user_path(current_user.id), class: :greeting__link%> </h1>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%= render partial: 'prototypes/prototype', collection: @prototypes %>
    </div>
  </div>
</main>
